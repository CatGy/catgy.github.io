<div id="carousselImages" class="carousel carousel-dark slide">
  <div class="carousel-indicators">
    <button type="button" data-bs-target="#carousselImages" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
    <button type="button" data-bs-target="#carousselImages" data-bs-slide-to="1" aria-label="Slide 2"></button>
    <button type="button" data-bs-target="#carousselImages" data-bs-slide-to="2" aria-label="Slide 3"></button>
  </div>
  <div class="carousel-inner">
    <% for (var i = 0; i < data.length; i++ ) { %>
    <div class="carousel-item active">
        <a href="<%= data[i].link %>">
            <img src="<%= data[i].imgSrc %>" class="d-block w-100" alt="<%= data[i].imgAlt %>">
            <div class="carousel-caption d-none d-md-block">
                <h5><%= data[i].label %></h5>
                <p><%= data[i].text %></p>
            </div>
        </a>
    </div>
    <% } %>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carousselImages" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carousselImages" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>